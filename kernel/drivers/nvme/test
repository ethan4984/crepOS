    uint16_t            max_power;  /* centiwatts */
    uint8_t         rsvd2;
    uint8_t         flags;
    uint32_t            entry_lat;  /* microseconds */
    uint32_t            exit_lat;   /* microseconds */
    uint8_t         read_tput;
    uint8_t         read_lat;
    uint8_t         write_tput;
    uint8_t         write_lat;
    uint16_t            idle_power;
    uint8_t         idle_scale;
    uint8_t         rsvd19;
    uint16_t            active_power;
    uint8_t         active_work_scale;
    uint8_t         rsvd23[9];
